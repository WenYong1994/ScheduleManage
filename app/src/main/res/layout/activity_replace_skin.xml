<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/m_root_view"
    tools:context=".activity.replaseskin.ReplaceSkinActivity">

    <RelativeLayout
        android:id="@+id/rel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        >

        <TextView
            android:id="@+id/tv"
            app:isSupport="true"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hellow andorid"
            android:layout_marginTop="12dp"
            android:textSize="20dp"
            android:textColor="@color/color_text_view_color"
            android:paddingLeft="100dp"
            />


        <Button
            android:id="@+id/btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="一键换肤"
            android:layout_marginTop="50dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:id="@+id/img"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="200dp"
            android:src="@mipmap/ic_launcher"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            />

        <FrameLayout
            android:id="@+id/bg_view"
            android:isSupport="true"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:background="@color/color_view_bg"
            />

        <com.jeek.calendar.widget.calendar.test.MyTestView
            android:id="@+id/bg_test"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignParentRight="true"
            app:test_background="@color/test_color"
            app:isSupport="true" />


    </RelativeLayout>










</FrameLayout>